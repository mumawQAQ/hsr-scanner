    },
    {
      "name": "ocr",
      "class": "app.core.model_impls.ocr_model.OCRModel"
    },
    {
      "name": "relic_matcher",
      "class": "app.core.model_impls.relic_matcher_model.RelicMatcherModel",
      "params": {
        "relic_sets_path": "assets/relic/relic_sets.json",
        "relic_main_stats_path": "assets/relic/relic_main_stats.json",
        "relic_sub_stats_path": "assets/relic/relic_sub_stats.json"
      }
    },
    {
      "name": "relic_rating",
      "class": "app.core.model_impls.relic_rating_model.RelicRatingModel",
      "params": {
        "global_state_manager": "app.core.managers.model_manager.GlobalStateManager"
      }
    },
    {
      "name": "yolo",
      "class": "app.core.model_impls.yolo_model.YOLOModel",
      "params": {
        "model_path": "assets/yolo_model/best.pt"
